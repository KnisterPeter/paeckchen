{
  "name": "paeckchen-cli",
  "version": "0.4.0",
  "description": "paeckchen command line interface",
  "main": "./dist/src/index.js",
  "bin": {
    "paeckchen": "./dist/src/index.js"
  },
  "files": [
    "dist/src"
  ],
  "scripts": {
    "clean": "rimraf dist .nyc_output coverage",
    "prelinter": "npm run build",
    "linter": "tslint --project ./tsconfig.json",
    "prebuild": "npm run clean",
    "build": "tsc --sourceMap --skipLibCheck",
    "prebuild:watch": "npm run clean",
    "build:watch": "tsc --sourceMap --skipLibCheck --watch",
    "pretest": "npm run build",
    "test": "ava",
    "test:watch": "ava --watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "preversion": "npm test",
    "version": "npm run build && conventional-changelog -p angular -v -r 0 > CHANGELOG.md && git add -A .",
    "postversion": "git push && git push --tags",
    "prepublish": "not-in-install && npm run test || echo 'In install cycle, skipping prepublish'",
    "precoverage": "npm run clean && tsc --inlineSourceMap --skipLibCheck && npm run linter",
    "coverage": "nyc --reporter lcov --all ava"
  },
  "repository": {
    "type": "git",
    "url": "paeckchen/paeckchen"
  },
  "author": {
    "name": "Markus Wolf",
    "email": "knister.peter@shadowrun-clan.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paeckchen/paeckchen/issues"
  },
  "homepage": "https://github.com/paeckchen/paeckchen#readme",
  "keywords": [
    "javascript",
    "bundling",
    "packaging"
  ],
  "devDependencies": {
    "@types/debug": "~0.0.29",
    "@types/meow": "~3.6.0",
    "@types/minimist": "~1.1.29",
    "@types/node": "~6.0.52",
    "ava": "0.15.2",
    "coveralls": "~2.11.15",
    "execa": "~0.5.0",
    "in-publish": "~2.0.0",
    "nyc": "~10.0.0",
    "rimraf": "~2.5.4",
    "source-map-support": "~0.4.6",
    "tslint": "~4.1.1",
    "typescript": "~2.1.4"
  },
  "dependencies": {
    "debug": "~2.4.5",
    "meow": "~3.7.0",
    "paeckchen-core": "0.4.0",
    "terminal-kit": "~0.25.6"
  },
  "publishConfig": {
    "tag": "next"
  },
  "ava": {
    "files": [
      "dist/test/**/*-test.js"
    ],
    "source": [
      "dist/src/**/*.js"
    ],
    "require": [
      "source-map-support/register"
    ]
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "coverage",
      "dist/test",
      "test"
    ]
  }
}
