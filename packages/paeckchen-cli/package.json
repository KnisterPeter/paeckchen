{
  "name": "paeckchen-cli",
  "version": "0.4.0",
  "description": "paeckchen command line interface",
  "main": "./dist/src/index.js",
  "bin": {
    "paeckchen": "./dist/src/index.js"
  },
  "files": [
    "dist/src"
  ],
  "scripts": {
    "clean": "rimraf dist .nyc_output coverage",
    "linter": "tslint --project ./tsconfig.json --type-check",
    "prebuild": "npm run clean",
    "build": "tsc --sourceMap --skipLibCheck",
    "prebuild:watch": "npm run clean",
    "build:watch": "tsc --sourceMap --skipLibCheck --watch",
    "pretest": "npm run build",
    "test": "ava",
    "test:watch": "ava --watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "preversion": "npm test",
    "version": "npm run build && conventional-changelog -p angular -v -r 0 > CHANGELOG.md && git add -A .",
    "postversion": "git push && git push --tags",
    "prepublish": "not-in-install && npm run test || echo 'In install cycle, skipping prepublish'",
    "precoverage": "npm run clean && tsc --inlineSourceMap --skipLibCheck && npm run linter",
    "coverage": "nyc --reporter lcov --all ava"
  },
  "repository": {
    "type": "git",
    "url": "paeckchen/paeckchen"
  },
  "author": {
    "name": "Markus Wolf",
    "email": "knister.peter@shadowrun-clan.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paeckchen/paeckchen/issues"
  },
  "homepage": "https://github.com/paeckchen/paeckchen#readme",
  "keywords": [
    "javascript",
    "bundling",
    "packaging"
  ],
  "devDependencies": {
    "@types/debug": "0.0.28",
    "@types/minimist": "1.1.28",
    "@types/node": "4.0.30",
    "ava": "0.15.2",
    "coveralls": "2.11.12",
    "execa": "0.4.0",
    "in-publish": "2.0.0",
    "nyc": "8.1.0",
    "rimraf": "2.5.4",
    "source-map-support": "0.4.2",
    "tslint": "4.0.1",
    "typescript": "2.0.0"
  },
  "dependencies": {
    "debug": "2.2.0",
    "meow": "3.7.0",
    "paeckchen-core": "0.4.0",
    "terminal-kit": "0.24.20"
  },
  "publishConfig": {
    "tag": "next"
  },
  "ava": {
    "files": [
      "dist/test/**/*-test.js"
    ],
    "source": [
      "dist/src/**/*.js"
    ],
    "require": [
      "source-map-support/register"
    ]
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "coverage",
      "dist/test",
      "test"
    ]
  }
}
