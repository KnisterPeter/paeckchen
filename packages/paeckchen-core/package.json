{
  "name": "paeckchen-core",
  "version": "0.4.0",
  "description": "paeckchen core library",
  "main": "dist/src/index.js",
  "files": [
    "dist/src",
    "index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf dist .nyc_output coverage",
    "prebuild": "npm run clean",
    "build": "tsc --sourceMap",
    "prebuild:watch": "npm run clean",
    "build:watch": "tsc --sourceMap --watch",
    "pretest": "npm run build",
    "test": "ava",
    "test:watch": "ava --watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "preversion": "npm test",
    "version": "npm run build && conventional-changelog -p angular -v -r 0 > CHANGELOG.md && git add -A .",
    "postversion": "git push && git push --tags",
    "prepublish": "not-in-install && npm run test || echo 'In install cycle, skipping prepublish'",
    "prelinter": "npm run build",
    "linter": "tslint --project ./tsconfig.json --type-check",
    "precoverage": "npm run clean && tsc --inlineSourceMap && npm run linter",
    "coverage": "nyc --reporter lcov --all ava"
  },
  "repository": {
    "type": "git",
    "url": "paeckchen/paeckchen"
  },
  "author": {
    "name": "Markus Wolf",
    "email": "knister.peter@shadowrun-clan.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paeckchen/paeckchen/issues"
  },
  "homepage": "https://github.com/paeckchen/paeckchen#readme",
  "devDependencies": {
    "@types/browser-resolve": "0.0.4",
    "@types/common-tags": "1.2.5",
    "@types/lodash": "4.14.43",
    "@types/mkdirp": "0.3.29",
    "@types/node": "4.0.30",
    "ava": "0.17.0",
    "common-tags": "1.4.0",
    "coveralls": "2.11.15",
    "in-publish": "2.0.0",
    "lodash": "4.17.2",
    "nyc": "10.0.0",
    "rimraf": "2.5.4",
    "source-map-support": "0.4.6",
    "tslint": "4.0.2",
    "typescript": "2.1.4"
  },
  "dependencies": {
    "@types/acorn": "1.0.31",
    "@types/estree": "0.0.34",
    "acorn": "4.0.3",
    "ast-types": "0.9.2",
    "browser-resolve": "1.11.2",
    "chokidar": "1.6.1",
    "escodegen": "1.8.1",
    "mkdirp": "0.5.1",
    "node-libs-browser": "2.0.0",
    "paeckchen-sorcery": "0.1.1"
  },
  "publishConfig": {
    "tag": "next"
  },
  "ava": {
    "files": [
      "dist/test/**/*-test.js"
    ],
    "source": [
      "dist/src/**/*.js"
    ],
    "require": [
      "source-map-support/register"
    ]
  },
  "nyc": {
    "exclude": [
      "node_modules",
      "dist/src/index.js",
      "dist/test",
      "coverage",
      "test"
    ]
  }
}
