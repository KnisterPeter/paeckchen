{
  "name": "paeckchen",
  "version": "0.1.0",
  "description": "",
  "main": "dist/src/index.js",
  "files": [
    "dist",
    "index.d.ts",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc --sourceMap",
    "pretest": "npm run build",
    "test": "mocha --require source-map-support/register --recursive dist/test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "preversion": "npm test",
    "version": "npm run build && conventional-changelog -p angular -v -r 0 > CHANGELOG.md && git add -A .",
    "postversion": "git push && git push --tags",
    "prepublish": "not-in-install && npm run build || echo 'In install cycle, skipping prepublish'",
    "precoverage": "npm run clean && tsc --inlineSourceMap",
    "coverage": "nyc --reporter lcov ./node_modules/.bin/mocha --recursive dist/test --all"
  },
  "repository": {
    "type": "git",
    "url": "KnisterPeter/paeckchen"
  },
  "author": {
    "name": "Markus Wolf",
    "email": "knister.peter@shadowrun-clan.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knisterpeter/paeckchen/issues"
  },
  "homepage": "https://github.com/knisterpeter/paeckchen#readme",
  "devDependencies": {
    "chai": "3.5.0",
    "coveralls": "2.11.9",
    "cz-conventional-changelog": "1.1.5",
    "in-publish": "2.0.0",
    "mocha": "2.4.5",
    "nyc": "6.2.1",
    "rimraf": "2.5.2",
    "source-map-support": "0.4.0",
    "tslint": "3.7.1",
    "typescript": "1.8.9"
  },
  "dependencies": {
    "acorn": "3.1.0",
    "ast-types": "0.8.16",
    "astring": "0.6.1",
    "minimist": "1.2.0"
  },
  "publishConfig": {
    "tag": "next"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "coverage",
      "dist/test",
      "test"
    ]
  }
}
